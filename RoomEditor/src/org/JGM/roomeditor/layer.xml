<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.1//EN" "http://www.netbeans.org/dtds/filesystem-1_1.dtd">
<filesystem>
    <folder name="Loaders">
        <folder name="text">
            <folder name="x-gmjroom">
                <folder name="Actions">
                    <file name="org-JGM-roomeditor-RoomOpen.instance"/>
                </folder>
            </folder>
            <folder name="x-actor">
                <folder name="Actions">
                    <file name="org-gjava-actoreditor-ActorOpen.instance"/>
                </folder>
            </folder>
            
        </folder>
    </folder>
    
    
    <folder name="RoomEditorPalette">
        <folder name="Actors">
            <file name="act_1.actor" url="org-JGM-roomeditor-palette-roompalette.xml"/>
        </folder>
        <folder name="Tiles">
            <file name="image.png" url="roompalette.xml"/>
        </folder>
    </folder>
    
    
    <folder name="Services">
        <folder name="AutoupdateType">
            <file name="my_au_type.settings" url="my_au_type.settings">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="org.JGM.roomeditor.Bundle"/>
                <attr name="url" urlvalue="http://autoupdate.netbeans.org/nbm/dev_1.6_.xml"/>
            </file>
        </folder>
    </folder>
</filesystem>
