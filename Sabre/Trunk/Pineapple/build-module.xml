<project name="PineappleModule" default="default" basedir="..">
    <description>Performs some useful tasks</description>
    
    <target name="makemodule">
    
        <mkdir dir="Core/build/"/>
        <mkdir dir="Core/build/Plugins"/>
        <mkdir dir="Core/dist/"/>
        <mkdir dir="Core/dist/Plugins"/>
    
        <copy file="Pineapple/dist/Pineapple.jar" todir="Core/build/Plugins"/>
        <copy file="Pineapple/dist/Pineapple.jar" todir="Core/dist/Plugins"/>
    
    </target>
</project>
